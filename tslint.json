{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-eslint-rules"],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "always",
          "typeLiterals": "always"
        },
        "esSpecCompliant": true
      }
    ],
    "member-ordering": false,
  }
}
